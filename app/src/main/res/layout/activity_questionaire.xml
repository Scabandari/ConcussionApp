<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_questionaire"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context="com.example.concussionapp.Questionaire">

    <include
        android:id="@+id/tool_bar"
        layout="@layout/tool_bar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="0dp"
        android:layout_width="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="16dp"
        android:background="@color/colorPrimaryDark"
        android:textColor="@color/colorAccent"
        android:text="Symptom Evaluation"
        android:textSize="14sp"
        tools:layout_editor_absoluteY="5dp"
        tools:layout_editor_absoluteX="30dp" />


    <Button
        android:id="@+id/submitquiz"
        android:layout_width="190dp"
        android:layout_height="48dp"
        android:text="Submit"
        android:background="@color/colorPrimaryDark"
        android:textColor="@color/colorAccent"
        tools:layout_editor_absoluteX="80dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar5"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintVertical_bias="0.96" />

    <RatingBar
        android:id="@+id/ratingBar1"
        android:layout_width="232dp"
        android:layout_height="57dp"
        android:rating="5"
        android:stepSize="0.1"
        android:numStars="5"
        android:isIndicator="true"
        android:scaleX="0.65"
        android:scaleY="0.65"
        style="@style/AppTheme"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintBottom_toTopOf="@+id/Neckpain_tv" />

    <RatingBar
        android:id="@+id/ratingBar1"
        android:layout_width="232dp"
        android:layout_height="57dp"
        android:rating="5"
        android:stepSize="0.1"
        android:numStars="5"
        android:isIndicator="true"
        android:scaleX="0.65"
        android:scaleY="0.65"
        tools:layout_editor_absoluteY="54dp"
        tools:layout_editor_absoluteX="66dp" />

    <RatingBar
        android:id="@+id/ratingBar2"
        android:layout_width="232dp"
        android:layout_height="57dp"
        android:rating="5"
        android:stepSize="0.1"
        android:numStars="5"
        android:isIndicator="true"
        android:scaleX="0.65"
        android:scaleY="0.65"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        android:layout_marginBottom="8dp"
        app:layout_constraintHorizontal_bias="0.44" />

    <RatingBar
        android:id="@+id/ratingBar3"
        android:layout_width="232dp"
        android:layout_height="57dp"
        android:rating="5"
        android:stepSize="0.1"
        android:numStars="5"
        android:isIndicator="true"
        android:scaleX="0.65"
        android:scaleY="0.65"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Neckpain_tv"
        app:layout_constraintHorizontal_bias="0.45" />

    <RatingBar
        android:id="@+id/ratingBar4"
        android:layout_width="232dp"
        android:layout_height="57dp"
        android:rating="5"
        android:stepSize="0.1"
        android:numStars="5"
        android:isIndicator="true"
        android:scaleX="0.65"
        android:scaleY="0.65"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.42"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        android:layout_marginBottom="8dp" />

    <RatingBar
        android:id="@+id/ratingBar5"
        android:layout_width="232dp"
        android:layout_height="57dp"
        tools:layout_editor_absoluteY="435dp"
        android:rating="5"
        android:stepSize="0.1"
        android:numStars="5"
        android:isIndicator="true"
        android:scaleX="0.65"
        android:scaleY="0.65"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.41" />

    <TextView
        android:text="Headache "
        android:layout_width="110dp"
        android:layout_height="17dp"
        tools:layout_editor_absoluteX="66dp"
        android:id="@+id/Headache_tv"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar1"
        android:layout_marginBottom="8dp" />

    <TextView
        android:text="Pressure in Head "
        android:layout_width="106dp"
        android:layout_height="16dp"
        tools:layout_editor_absoluteY="109dp"
        tools:layout_editor_absoluteX="66dp"
        android:id="@+id/Pressure_tv"
        android:textColor="@color/colorAccent" />

    <TextView
        android:text="Neck Pain "
        android:layout_width="107dp"
        android:layout_height="21dp"
        tools:layout_editor_absoluteY="174dp"
        tools:layout_editor_absoluteX="64dp"
        android:id="@+id/Neckpain_tv"
        android:textColor="@color/colorAccent" />

    <TextView
        android:text="Diziness"
        android:layout_width="56dp"
        android:layout_height="19dp"
        tools:layout_editor_absoluteX="69dp"
        android:id="@+id/textView5"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar2" />

    <TextView
        android:text="Don't feel right "
        android:layout_width="105dp"
        android:layout_height="18dp"
        tools:layout_editor_absoluteX="67dp"
        android:id="@+id/textView6"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar4"
        android:layout_marginBottom="8dp" />

    <TextView
        android:text="Nervous Anxious"
        android:layout_width="103dp"
        android:layout_height="19dp"
        tools:layout_editor_absoluteX="66dp"
        android:id="@+id/textView7"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar5" />


</android.support.constraint.ConstraintLayout>